<label class="gl-label">Đến: </label>
<input onchange="dayend()" type="text" id="dayend" onfocus="(this.type='date')" onblur="(this.type='text')" class="input-text input-text--primary-style" min="@String.Format("{0:yyyy-MM-dd}", ViewBag.Start.AddDays(1))" max="@String.Format("{0:yyyy-MM-dd}",DateTime.UtcNow.AddHours(7))" value=@String.Format("{0:yyyy-MM-dd}",ViewBag.End)>

